extends admin

block content
    h2 Dashboard
    br
    br
    table.table.tbale-striped
        tr
            th ID
            th Porject Name
            th Service
            th
            th
        each row, i in rows
            tr
                td #{row.id}
                td #{row.title}
                td #{row.service}
                td
                    a.btn.btn-primary(href='/admin/edit/#{row.id}') Edit
                td
                    a.btn.btn-danger(href='#' class='deleteProject' date-id="#{row.id}") Delete
script(src='vender/jquery/jquery.js')
script(src='javascripts/dashboard.js')